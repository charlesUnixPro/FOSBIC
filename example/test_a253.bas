140 REM KLASSIFIZIERUNG DER AKTIVITAETEN
200 FOR K=1 TO N
210   LET A(K)=K
220 NEXT K
230 FOR K2=1 TO N
240   LET K3=0
250   FOR K=1 TO N-K2
260     LET A1=A(K)
270     LET A2=A(K+1)
280     IF I(A1)<I(A2) GOTO 340
290     IF I(A1)>I(A2) GOTO 310
300     IF J(A1)<J(A2) GOTO 340
310     LET K3=K3+1
320     LET A(K)=A2
330     LET A(K+1)=A1
340   NEXT K
350   IF K3=0 GOTO 380
360 NEXT K2
370 REM INITIALISIERUNG
380 FOR K=1 TO N
390   LET T(K)=0
400   LET R(K)=0
410 NEXT K
420 REM VORWAERTSSPEICHERUNG
430 FOR K=1 TO N
440   LET I1=I(A(K))
450   LET J1=J(A(K))
460   LET M=T(I1)+D(A(K))
470   IF T(J1)=0 THEN 490
480   IF T(J1)>=M GOTO 500
490   LET T(J1)=M
500 NEXT K
510 REM RUECKWAERTSRECHNUNG
520 LET R(J(A(N)))=T(J(A(N)))
530 FOR K=N TO 1 STEP -1
540   LET I1=I(A(K))
550   LET M=R(J(A(K)))-D(A(K))
560   IF R(I1)=0 GOTO 580
570   IF R(I1)<=M GOTO 590
