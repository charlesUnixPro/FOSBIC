10  REM SORTIERPROGRAMM
20  DIM X(100)
30  READ N
40  FOR I=1 TO N STEP 1
50  READ X(I)
60  NEXT I
70  FOR I=1 TO N-1
80  FOR J=I+1 TO N
90  IF X(I) <= X(J) THEN 130
100 LET T = X(I)
110 LET X(I) = X(J)
120 LET X(J) = T
130 NEXT J
140 NEXT I
150 FOR I=1 TO N
160 PRINT X(I)
170 NEXT I
180 DATA 12
190 DATA 10,-5,5,9,8,-7,20,3,5,8,9,-99,99,20,12
200 END